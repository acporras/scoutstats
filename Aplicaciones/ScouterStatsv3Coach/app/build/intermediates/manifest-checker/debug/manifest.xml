        <activity android:name="com.scouterperu.scouterstatsv3coach.PagarActivity" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.scouterperu.scouterstatsv3coach.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>